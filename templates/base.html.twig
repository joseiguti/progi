<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Bid calculation{% endblock %}</title>
    <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
    {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
    {% endblock %}

    {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
    {% endblock %}

    {% if app.request.attributes.get('_route') == 'app_bid_calculation' %}
        {{ encore_entry_script_tags('custom') }}
    {% endif %}
</head>
<body>
    <nav class="navbar bg-body-tertiary bg-black navbar-light">
        <div class="container">
            <a class="navbar-brand text-white" href="#">
                <img src="https://progi.com/wp-content/uploads/2018/02/Progi_forme_99x45.png" width="50" alt="progi">
                Bid Calculation
            </a>
        </div>
    </nav>
    {% block body %}{% endblock %}
</body>
</html>